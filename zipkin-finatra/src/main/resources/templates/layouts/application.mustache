      <script type="text/javascript" src="/public/js/zipkin-onebox.js"></script>
      <script type="text/javascript" src="/public/js/zipkin-trace-dependency.js"></script>
      <script type="text/javascript" src="/public/js/zipkin-trace-summary.js"></script>

      <script type="text/javascript" src="/public/js/application.js"></script>
      <script type="text/javascript" src="/public/js/application-index.js"></script>
      <script type="text/javascript" src="/public/js/application-show.js"></script>
      <script type="text/javascript" src="/public/js/application-static.js"></script>
      <script>
        var root_url="{{rootUrl}}";
      </script>

      {{#environment}}
      {{#production}}
      <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-30739707-1']);
        _gaq.push(['_trackPageview']);
        (function() {
          var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
          ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
          var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
      </script>
      {{/production}}
      {{/environment}}

      <title>Zipkin</title>
  </head>
  <body id="{{controllerName}}_{{actionName}}">
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a href="{{rootUrl}}" class="brand">
             Zipkin<span class="muted" style="font-size: .75em; padding-left: 10px;">Investigate system behavior</span>
          </a>
          <ul class="nav nav-bar js-zipkin-navbar">
            <li class="active"><a href="#"><i class="icon-search icon-white"></i> Find a trace</a><li>
          </ul>
          <div class="pull-right">
            <div class="">
              <a class="adjust-clock-skew-tooltip" href="#" rel="tooltip" data-original-title="Clock skew adjustment: Enabled">
                <button class="btn adjust-clock-skew-btn btn-info" value="true"><i class="icon-time"></i></button>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="content">
        {{body}}
      </div>
    </div>
  </body>
</html>